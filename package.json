{
  "name": "devenv",
  "description": "",
  "version": "0.1.0",
  "scripts": {
    "watch:server": "browser-sync start -s ./dist/ --files ./dist/ reload",
    "watch:pug": "pug -w ./src/html/ -b ./src/html/ -P ./src/html/ -o ./dist/ -s",
    "watch:webpack": "webpack -w --env.development --config webpack.config.babel.js",
    "build:clean": "rimraf build",
    "build:pug": "pug ./src/html/ -b ./src/html/ -P ./src/html/ -o ./build/ -s",
    "build:webpack": "webpack --env.production --config webpack.config.babel.js",
    "start": "run-p watch:*",
    "build": "run-p build:*"
  },
  "author": "Hiromi Kozai",
  "license": "ISC",
  "homepage": "https://www.facebook.com/hiromir13",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.5",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "latest",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-calc": "latest",
    "postcss-cli": "latest",
    "postcss-css-variables": "latest",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-discard-duplicates": "latest",
    "postcss-import": "latest",
    "postcss-loader": "latest",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "latest",
    "postcss-short": "latest",
    "pug-cli": "github:pugjs/pug-cli#master",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "latest",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  },
