{
  "name": "sfit-lp",
  "description": "",
  "version": "0.1.0",
  "config": {
    "srcpath": "./src/",
    "dev": "./dist/",
    "build": "./build/"
  },
  "scripts": {
    "watch:server": "browser-sync start -s %npm_package_config_dev% --files %npm_package_config_dev% reload",
    "watch:pug": "pug -w %npm_package_config_srcpath%html/ -b %npm_package_config_srcpath%html/ -P %npm_package_config_srcpath%html/ -o %npm_package_config_dev% -s",
    "watch:webpack": "webpack -w --env.development --config webpack.config.babel.js",
    "build:pug": "pug %npm_package_config_srcpath%html/ -b %npm_package_config_srcpath%html/ -P %npm_package_config_srcpath%html/ -o %npm_package_config_build% -s",
    "build:webpack": "webpack --env.production --config webpack.config.babel.js",
    "start": "run-p watch:*",
    "build": "run-p build:*"
  },
  "author": "Hiromi Kozai",
  "license": "ISC",
  "homepage": "https://www.facebook.com/hiromir13",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.3",
    "browser-sync": "^2.18.13",
    "clean-css-cli": "^4.0.12",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "imagemin-optipng": "^5.2.1",
    "imagemin-webpack-plugin": "^2.3.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "latest",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.14.6",
    "postcss-calc": "latest",
    "postcss-cli": "latest",
    "postcss-css-variables": "latest",
    "postcss-cssnext": "^3.1.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-discard-duplicates": "latest",
    "postcss-import": "latest",
    "postcss-loader": "latest",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "latest",
    "postcss-short": "latest",
    "pug-cli": "github:pugjs/pug-cli#master",
    "sass-loader": "^7.1.0",
    "style-loader": "latest",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
